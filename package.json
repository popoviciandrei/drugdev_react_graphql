{
  "name": "frontend-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "apollo-boost": "^0.4.3",
    "apollo-server": "^2.4.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql.macro": "^1.4.2",
    "lowdb": "^1.0.0",
    "nanoid": "^3.1.31",
    "react": "^16.8.1",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.3",
    "typescript": "3.3.1",
    "xstate": "^4.6.7"
  },
  "scripts": {
    "download-schema": "apollo service:download --endpoint=http://localhost:3001/ ./src/resources/schema.json",
    "generate-types": "apollo client:codegen --localSchemaFile=./src/resources/schema.json --target=typescript --includes=./src/graphql/**/*.graphql --addTypename --outputFlat ./src/resources/gql-types.ts",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql": "node server.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "@types/graphql": "^14.2.2",
    "@types/jest": "24.0.0",
    "@types/node": "10.12.21",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "^4.3.4",
    "babel-plugin-import-graphql": "^2.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs,tsx,ts}"
    ]
  }
}
